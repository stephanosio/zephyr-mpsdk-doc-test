		(((<ref refid="stdint_8h_1a0a8582351ac627ee8bde2973c825e47f" kindref="member">uint32_t</ref>)(green) &amp; 0xff) &lt;&lt; 8) | \
		((<ref refid="stdint_8h_1a0a8582351ac627ee8bde2973c825e47f" kindref="member">uint32_t</ref>)(blue) &amp; 0xff))</initializer>
