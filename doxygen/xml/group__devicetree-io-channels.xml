      <memberdef kind="define" id="group__devicetree-io-channels_1ga290c912d0a96ba65bb44341a783fac19" prot="public" static="no">
        <name>DT_IO_CHANNELS_INPUT_BY_IDX</name>
        <param><defname>node_id</defname></param>
