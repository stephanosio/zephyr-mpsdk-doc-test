        <name>EC_HOST_CMD_HANDLER</name>
        <param><defname>_function</defname></param>
        <param><defname>_id</defname></param>
        <param><defname>_version_mask</defname></param>
        <param><defname>_request_type</defname></param>
        <param><defname>_response_type</defname></param>
